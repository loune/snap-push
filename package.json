{
  "name": "snap-push",
  "version": "2.0.1",
  "description": "Deliver static website files to Amazon S3, Azure or GCP.",
  "main": "dist/push.js",
  "engines": {
    "node": ">=8.10"
  },
  "bin": {
    "snap-push": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "yarn lint && yarn env-cmd --silent jest",
    "lint": "npx eslint --ext .js,.ts src",
    "build": "rm -rf dist && mkdir dist && yarn tsc && chmod +x dist/cli.js",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loune/snap-push.git"
  },
  "keywords": [
    "push",
    "snapshot",
    "static",
    "website",
    "AWS",
    "azure",
    "gcp"
  ],
  "author": "Loune Lam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loune/snap-push/issues"
  },
  "homepage": "https://github.com/loune/snap-push#readme",
  "peerDependencies": {
    "@azure/storage-blob": "^12.1.1",
    "@google-cloud/storage": "^4.7.0",
    "aws-sdk": "^2.663.0"
  },
  "peerDependenciesMeta": {
    "@azure/storage-blob": {
      "optional": true
    },
    "@google-cloud/storage": {
      "optional": true
    },
    "aws-sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@azure/storage-blob": "^12.1.1",
    "@google-cloud/storage": "^4.7.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^11.13.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "aws-sdk": "^2.663.0",
    "azurite": "^3.7.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "prettier": "^1.17.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "fast-glob": "^2.2.6",
    "mime": "^2.4.2",
    "p-limit": "^2.2.0",
    "yargs": "^13.2.4"
  }
