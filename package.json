{
  "name": "snap-push",
  "version": "4.1.0",
  "description": "Deliver static website files to Amazon S3, Azure or GCP.",
  "main": "dist/push.js",
  "engines": {
    "node": ">=8.10"
  },
  "bin": {
    "snap-push": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "yarn lint && yarn env-cmd --silent jest --detectOpenHandles",
    "lint": "npx eslint --ext .js,.ts src",
    "build": "rimraf dist && mkdir dist && yarn tsc && chmod +x dist/cli.js",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loune/snap-push.git"
  },
  "keywords": [
    "push",
    "snapshot",
    "static",
    "website",
    "AWS",
    "azure",
    "gcp"
  ],
  "author": "Loune Lam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loune/snap-push/issues"
  },
  "homepage": "https://github.com/loune/snap-push#readme",
  "peerDependencies": {
    "@azure/storage-blob": "^12.1.1",
    "@google-cloud/storage": "^5.3.0",
    "aws-sdk": "^2.663.0"
  },
  "peerDependenciesMeta": {
    "@azure/storage-blob": {
      "optional": true
    },
    "@google-cloud/storage": {
      "optional": true
    },
    "aws-sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@azure/storage-blob": "^12.1.1",
    "@google-cloud/storage": "^5.3.0",
    "@types/jest": "^27.0.2",
    "@types/mime": "^2.0.3",
    "@types/node": "^12",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "aws-sdk": "^2.663.0",
    "azurite": "^3.10.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "fast-glob": "^3.2.4",
    "mime": "^2.4.2",
    "p-limit": "^3.0.2",
    "yargs": "^17.2.1"
